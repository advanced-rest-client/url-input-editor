{
  "name": "url-input-editor",
  "version": "0.2.8",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An element that is responsible for displaying and manipulating the reuqest URL",
  "devDependencies": {
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "url-input-editor.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint url-input-editor.html url-detailed-editor.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"url-input-editor:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/url-input-editor.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/url-input-editor/issues",
    "email": "arc@mulesoft.com"
  }
}
